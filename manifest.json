{
  "manifest_version": 3,
  "name": "Focus Tracker MVP",
  "description": "Rastreador simples de tempo de foco MV3.",
  "version": "1.0.0",
  "icons": {
    "16": "icons/icon16.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "src/background/service-worker.js"
  },
  "permissions": ["storage", "alarms"]
}
